<h1><%=@task.title%></h1><%= link_to "Edit Task", "#{@task.id}/edit" %>
<%= link_to "Delete",
  task_path(@task),
  data: {turbo_method: :delete, turbo_confirm: "Delete #{@task.title}?"}
%>


<p><%= @task.details %></p>
<p><%= @task.completed %></p>

<p><%= link_to "Back to overview", tasks_path %></p>
